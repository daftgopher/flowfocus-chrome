{
  "manifest_version": 2,

  "name": "f.low",
  "description": "Tracks how much time you spend on distracting websites",
  "version": "0.1",

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "f.low"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications",
    "https://ajax.googleapis.com/"
  ]
}